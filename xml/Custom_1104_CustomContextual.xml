				<top>260</top>
				<visible>!String.IsEmpty(Window(home).Property(movie_tmdbid)) | !String.IsEmpty(Window(home).Property(tvshow_tmdbid))</visible>
				<radiowidth>500</radiowidth>
				<include content="1104_button">
					<param name="icon" value="flags/ratings/trakt_monochrome.png" />
					<param name="label2" value="[I]Trakt Collection[/I]    [B]R e m o v e[/B]                          " />
				</include>
				<onup>405</onup><ondown>407</ondown>
				<onclick>Dialog.Close(1104,true)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(movie_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=collection_remove&amp;tmdb_id=$INFO[Window(home).Property(movie_tmdbid)]&amp;type=movie)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(tvshow_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=collection_remove&amp;tmdb_id=$INFO[Window(home).Property(tvshow_tmdbid)]&amp;type=tv)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(episodetvshow_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=collection_remove&amp;tmdb_id=$INFO[$INFO[Window(home).Property(episodetvshow_tmdbid)]&amp;type=episode&amp;season=$INFO[ListItem.Season]&amp;episode=$INFO[ListItem.Episode])</onclick>
			</control>
			
			<control type="radiobutton" id="408">
				<top>440</top>
				<visible>!String.IsEmpty(Window(home).Property(movie_tmdbid)) | !String.IsEmpty(Window(home).Property(tvshow_tmdbid))</visible>
				<radiowidth>500</radiowidth>
				<include content="1104_button">
					<param name="icon" value="flags/ratings/trakt_monochrome.png" />
					<param name="label2" value="[I]Trakt Watched History[/I]   [B]R e m o v e[/B]                          " />
				</include>
				<onup>407</onup><ondown>402</ondown>
				<onclick>Dialog.Close(1104,true)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(movie_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=history_remove&amp;tmdb_id=$INFO[Window(home).Property(movie_tmdbid)]&amp;type=movie)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(tvshow_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=history_remove&amp;tmdb_id=$INFO[Window(home).Property(tvshow_tmdbid)]&amp;type=tv)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(episodetvshow_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=history_remove&amp;tmdb_id=$INFO[$INFO[Window(home).Property(episodetvshow_tmdbid)]&amp;type=episode&amp;season=$INFO[ListItem.Season]&amp;episode=$INFO[ListItem.Episode])</onclick>
			</control>
