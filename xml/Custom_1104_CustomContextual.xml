<?xml version="1.0" encoding="UTF-8"?>
<window id="1104" type="dialog">
	<defaultcontrol always="true">400</defaultcontrol>
	
	<controls>
		
		<control type="group">
			<description>Heading</description>
			<include>Animation_OpenClose_FadeZoom</include>
			<control type="image">
				<texture colordiffuse="$VAR[color_global_dialog_background]">colors/white90.png</texture>
				<aspectratio>stretch</aspectratio>
			</control>
			<control type="label">
				<width>100%</width>
				<top>20%</top>
				<height>60</height>
				<font>montserrat_40_extrabold</font>
				<aligny>center</aligny>
				<align>center</align>
				<textoffsetx>5</textoffsetx>
				<textcolor>$VAR[color_global_dialog_accent]</textcolor>
				<label>$VAR[1104_header]</label>
			</control>
		</control>
		
		<control type="group" id="400">
			<height>100%</height>
			<left>40%</left>
			<top>30%</top>
			
			<include content="Animation_OpenClose_Slide">
				<param name="slide_start" value="1920,0" />
				<param name="slide_end" value="0,0" />
			</include>
			
			<control type="radiobutton" id="401">
				<description>embuary lookup</description>
				<visible>!String.IsEmpty(Window(home).Property(CastMemberLabel))</visible>
				<top>50</top>
				<radioposx>50</radioposx>
				<radiowidth>150</radiowidth>
				
				<include content="1104_button">
					<param name="height" value="200" />
					<param name="icon_height" value="150" />
					<param name="icon" value="icons/embinfowatersign.png" />
					<param name="label" value="                 [I]Search[/I]    [B]T M D B[/B]" />
				</include>
				<onup>402</onup><ondown>402</ondown>
				<onclick>RunScript(script.embuary.info,call=person,query='"$INFO[Window(home).Property(CastMemberLabel)]"')</onclick>
				<onclick>Dialog.Close(1104,true)</onclick>
				<onclick>ClearProperty(CastMemberLabel,home)</onclick>
			</control>
			
			<control type="radiobutton" id="403">
				<description>trakt wl add</description>
				<visible>!String.IsEmpty(Window(home).Property(movie_tmdbid)) | !String.IsEmpty(Window(home).Property(tvshow_tmdbid))</visible>
				<radioposx>50</radioposx>
				<radiowidth>150</radiowidth>
				
				<include content="1104_button">
					<param name="icon" value="flags/ratings/trakt_monochrome.png" />
					<param name="label" value="                 [I]Trakt Wathclist[/I]    [B]A d d[/B]" />
				</include>
				
				<onup>402</onup><ondown>404</ondown>
				<onclick>Dialog.Close(1104,true)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(movie_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=watchlist_add&amp;tmdb_id=$INFO[Window(home).Property(movie_tmdbid)]&amp;type=movie)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(tvshow_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=watchlist_add&amp;tmdb_id=$INFO[Window(home).Property(tvshow_tmdbid)]&amp;type=tv)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(episodetvshow_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=watchlist_add&amp;tmdb_id=$INFO[$INFO[Window(home).Property(episodetvshow_tmdbid)]&amp;type=episode&amp;season=$INFO[ListItem.Season]&amp;episode=$INFO[ListItem.Episode])</onclick>
			</control>
			
			<control type="radiobutton" id="405">
				<top>180</top>
				
				<visible>!String.IsEmpty(Window(home).Property(movie_tmdbid)) | !String.IsEmpty(Window(home).Property(tvshow_tmdbid))</visible>
				<height>75</height>
				<radioposx>50</radioposx>
				<radiowidth>150</radiowidth>

				<include content="1104_button">
					<param name="icon" value="flags/ratings/trakt_monochrome.png" />
					<param name="label" value="                 [I]Trakt Collection[/I]    [B]A d d[/B]" />
				</include>
				
				<onup>404</onup><ondown>406</ondown>
				<onclick>Dialog.Close(1104,true)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(movie_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=collection_add&amp;tmdb_id=$INFO[Window(home).Property(movie_tmdbid)]&amp;type=movie)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(tvshow_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=collection_add&amp;tmdb_id=$INFO[Window(home).Property(tvshow_tmdbid)]&amp;type=tv)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(episodetvshow_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=collection_add&amp;tmdb_id=$INFO[$INFO[Window(home).Property(episodetvshow_tmdbid)]&amp;type=episode&amp;season=$INFO[ListItem.Season]&amp;episode=$INFO[ListItem.Episode])</onclick>
			</control>
			
			<control type="radiobutton" id="407">
				<top>360</top>
				
				<visible>!String.IsEmpty(Window(home).Property(movie_tmdbid)) | !String.IsEmpty(Window(home).Property(tvshow_tmdbid))</visible>
				
				<radioposx>50</radioposx>
				<radiowidth>150</radiowidth>
				
				<include content="1104_button">
					<param name="icon" value="flags/ratings/trakt_monochrome.png" />
					<param name="label" value="                 [I]Trakt Watched History[/I]    [B]A d d[/B]" />
				</include>
				
				<onup>406</onup><ondown>408</ondown>
				<onclick>Dialog.Close(1104,true)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(movie_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=history_add&amp;tmdb_id=$INFO[Window(home).Property(movie_tmdbid)]&amp;type=movie)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(tvshow_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=history_add&amp;tmdb_id=$INFO[Window(home).Property(tvshow_tmdbid)]&amp;type=tv)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(episodetvshow_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=history_add&amp;tmdb_id=$INFO[$INFO[Window(home).Property(episodetvshow_tmdbid)]&amp;type=episode&amp;season=$INFO[ListItem.Season]&amp;episode=$INFO[ListItem.Episode])</onclick>
			</control>
		</control>
		
		<control type="group">
			<height>100%</height>
			<right>40%</right>
			<top>30%</top>
			
			<include content="Animation_OpenClose_Slide">
				<param name="slide_start" value="-1920,0" />
				<param name="slide_end" value="0,0" />
			</include>
			
			<control type="radiobutton" id="402">
				<top>300</top>
				<description>lib search</description>
				<visible>!String.IsEmpty(Window(home).Property(CastMemberLabel))</visible>
				<radiowidth>500</radiowidth>
		
				<include content="1104_button">
					<param name="height" value="200" />
					<param name="icon" value="icons/swanwatersign.png" />
					<param name="label2" value="[I]Search[/I]   [B]L I B R A R Y[/B]                            " />
				</include>
				
				<onup>401</onup><ondown>401</ondown>
				<onclick>Skin.SetString(SearchTerm,$INFO[Window(home).Property(CastMemberLabel)])</onclick>
				<onclick>ClearProperty(CastMemberLabel,home)</onclick>
				<onclick condition="Window.IsActive(movieinformation)">Dialog.Close(movieinformation,true)</onclick>
				<onclick condition="Window.IsActive(fullscreeninfo)">Dialog.Close(fullscreeninfo)</onclick>
				<onclick>Dialog.Close(1104,true)</onclick>
				<onclick>ActivateWindow(1120,return)</onclick>
			</control>
			
			<control type="radiobutton" id="404">
				<top>80</top>
				<visible>!String.IsEmpty(Window(home).Property(movie_tmdbid)) | !String.IsEmpty(Window(home).Property(tvshow_tmdbid))</visible>
				<radiowidth>500</radiowidth>
				<include content="1104_button">
					<param name="icon" value="flags/ratings/trakt_monochrome.png" />
					<param name="label2" value="[I]Trakt Wathclist[/I]    [B]R e m o v e[/B]                          " />
				</include>
				<onup>403</onup><ondown>405</ondown>
				<onclick>Dialog.Close(1104,true)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(movie_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=watchlist_remove&amp;tmdb_id=$INFO[Window(home).Property(movie_tmdbid)]&amp;type=movie)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(tvshow_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=watchlist_remove&amp;tmdb_id=$INFO[Window(home).Property(tvshow_tmdbid)]&amp;type=tv)</onclick>
				<onclick condition="!String.IsEmpty(Window(home).Property(episodetvshow_tmdbid))">RunPlugin(plugin://plugin.video.themoviedb.helper?info=details&amp;trakt=watchlist_remove&amp;tmdb_id=$INFO[$INFO[Window(home).Property(episodetvshow_tmdbid)]&amp;type=episode&amp;season=$INFO[ListItem.Season]&amp;episode=$INFO[ListItem.Episode])</onclick>
			</control>
			
