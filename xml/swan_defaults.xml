<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<!-- 
	<default type="ranges" />
	https://forum.kodi.tv/showthread.php?tid=343076&pid=2846526#pid2846526 
	
	https://github.com/xbmc/xbmc/blob/master/xbmc/guilib/GUIControlFactory.cpp
	
	https://github.com/xbmc/xbmc/tree/master/xbmc/guilib
	-->

	<default type="image">
		<texture />
		<align>center</align>
        <aligny>center</aligny>
    </default>
	<default type="multiimage">
		<aspectratio align="center">scale</aspectratio>
		<fadetime>800</fadetime>
		<timeperimage>5000</timeperimage>
		<randomize>true</randomize>
	</default>
	<default type="textbox">
		<align>left</align>
		<aligny>center</aligny>
		<textcolor>white</textcolor>
		<font>font13</font>
		<autoscroll delay="5000" time="5000" repeat="10000">true</autoscroll>
		<wrapmultiline>true</wrapmultiline>
	</default>
	<default type="label">
		<label />
		<align>left</align>
		<aligny>center</aligny>
		<font>font13</font>
		<textcolor>white</textcolor>
		<disabledcolor>$VAR[color_global_button_disabled]</disabledcolor>
		<selectedcolor>$VAR[color_global_button_selected]</selectedcolor>
		<scroll>false</scroll>
		<scrollspeed>40</scrollspeed>
	</default>
	<default type="fadelabel">
		<align>left</align>
		<font>font13</font>
		<textcolor>white</textcolor>
		<disabledcolor>$VAR[color_global_button_disabled]</disabledcolor>
	</default>
	<default type="grouplist">
		<scrolltime tween="quadratic" easing="in">400</scrolltime>
		<itemgap>0</itemgap>
		<orientation>vertical</orientation>
		<usecontrolcoords>true</usecontrolcoords>
	</default>
	<!-- scrollbar/sliderbackground.png ?-->
	<default type="scrollbar">
		<texturesliderbackground colordiffuse="$VAR[color_global_scrollbar_panel]">colors/white.png</texturesliderbackground>
		<texturesliderbar colordiffuse="$VAR[color_global_scrollbar_slider_nf]">colors/white.png</texturesliderbar>
		<texturesliderbarfocus colordiffuse="$VAR[color_global_scrollbar_slider_fo]">colors/white.png</texturesliderbarfocus>
		<textureslidernib />
		<textureslidernibfocus />
		<width>5</width>
		<showonepage>false</showonepage>
		<orientation>vertical</orientation>
		<onup>60</onup>
		<ondown>60</ondown>
	</default>
	<default type="button">
		<texturefocus colordiffuse="$VAR[color_global_button_panel_fo]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[color_global_button_panel_nf]">colors/white.png</texturenofocus>
		<height>50</height>
		<width>100%</width>
		<font>font27</font>
		<focusedcolor>$VAR[color_global_button_text_fo]</focusedcolor>
		<textcolor>$VAR[color_global_button_text_nf]</textcolor>
		<disabledcolor>$VAR[color_global_button_disabled]</disabledcolor>
		<invalidcolor>$VAR[color_global_button_invalid]</invalidcolor>
		<textoffsetx>40</textoffsetx>
		<aligny>center</aligny>
		<pulseonselect>no</pulseonselect>
	</default>
	
	
	<default type="togglebutton">
		<height>50</height>
		<texturefocus colordiffuse="$VAR[color_global_button_panel_fo]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[color_global_button_panel_nf]">colors/white.png</texturenofocus>
		<alttexturefocus colordiffuse="$VAR[color_global_button_panel_fo]">colors/white.png</alttexturefocus>
		<alttexturenofocus colordiffuse="$VAR[color_global_button_panel_nf]">colors/white.png</alttexturenofocus>
		<focusedcolor>$VAR[color_global_button_text_fo]</focusedcolor>
		<textcolor>$VAR[color_global_button_text_nf]</textcolor>
		<font>font13</font>
		<disabledcolor>$VAR[color_global_button_disabled]</disabledcolor>
		<pulseonselect>no</pulseonselect>
	</default>
	
	<default type="radiobutton">
		<label />
		<height>50</height>
		<top>0</top>
		<aligny>center</aligny>
		<textoffsetx>40</textoffsetx>
		<radiowidth>45</radiowidth>
		<radioheight>35</radioheight>
		<textureradioonfocus colordiffuse="$VAR[color_global_button_selected]">buttons/radioonfo.png</textureradioonfocus>
		<textureradioonnofocus colordiffuse="$VAR[color_global_button_selected]">buttons/radioonfo.png</textureradioonnofocus>
		<textureradioofffocus colordiffuse="$VAR[color_global_button_text_fo]">buttons/radioofffo.png</textureradioofffocus>
		<textureradiooffnofocus colordiffuse="$VAR[color_global_button_text_nf]">buttons/radioofffo.png</textureradiooffnofocus>
		<textureradioondisabled colordiffuse="disabled">buttons/radioon.png</textureradioondisabled>
		<textureradiooffdisabled colordiffuse="disabled">buttons/radiooff.png</textureradiooffdisabled>
		
		<texturefocus colordiffuse="$VAR[color_global_button_panel_fo]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[color_global_button_panel_nf]">colors/white.png</texturenofocus>
		
		<font>font27</font>
		<focusedcolor>$VAR[color_global_button_text_fo]</focusedcolor>
		<textcolor>$VAR[color_global_button_text_nf]</textcolor>
		<disabledcolor>$VAR[color_global_button_disabled]</disabledcolor>
		<invalidcolor>$VAR[color_global_button_invalid]</invalidcolor>
		<selectedcolor>$VAR[color_global_button_selected]</selectedcolor>
		<pulseonselect>no</pulseonselect>
	</default>
	
	<default type="spincontrol">
		<textureup colordiffuse="disabled">buttons/spinctrlup.png</textureup>
		<texturedown colordiffuse="disabled">buttons/spinctrldown.png</texturedown>
		<textureupfocus>buttons/spinctrlup.png</textureupfocus>
		<texturedownfocus>buttons/spinctrldown.png</texturedownfocus>
		<textureupdisabled colordiffuse="disabled">buttons/spinctrlup.png</textureupdisabled>
		<texturedowndisabled colordiffuse="disabled">buttons/spinctrldown.png</texturedowndisabled>
		<align>right</align>
		<width>77</width>
		<height>50</height>
		<font>font13</font>
		<reverse>yes</reverse>
		<focusedcolor>$VAR[color_global_button_text_fo]</focusedcolor>
		<textcolor>$VAR[color_global_button_text_nf]</textcolor>
		<disabledcolor>$VAR[color_global_button_disabled]</disabledcolor>
		<subtype>text</subtype>
		<textoffsetx>40</textoffsetx>
		<textoffsety>0</textoffsety>
		<pulseonselect>no</pulseonselect>
	</default>
	<default type="spincontrolex">
		<height>50</height>
		<textwidth>475</textwidth>
		<spinwidth>32</spinwidth>
		<spinheight>60</spinheight>
		<texturefocus colordiffuse="$VAR[color_global_button_panel_fo]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[color_global_button_panel_nf]">colors/white.png</texturenofocus>
		<textureup colordiffuse="disabled">buttons/spinctrlup.png</textureup>
		<texturedown colordiffuse="disabled">buttons/spinctrldown.png</texturedown>
		<textureupfocus>buttons/spinctrlup.png</textureupfocus>
		<texturedownfocus>buttons/spinctrldown.png</texturedownfocus>
		<textureupdisabled colordiffuse="disabled">buttons/spinctrlup.png</textureupdisabled>
		<texturedowndisabled colordiffuse="disabled">buttons/spinctrldown.png</texturedowndisabled>
		<font>font13</font>
		<focusedcolor>$VAR[color_global_button_text_fo]</focusedcolor>
		<textcolor>$VAR[color_global_button_text_nf]</textcolor>
		<disabledcolor>$VAR[color_global_button_disabled]</disabledcolor>
		<textoffsetx>40</textoffsetx>
		<aligny>center</aligny>
		<reverse>yes</reverse>
		<pulseonselect>no</pulseonselect>
	</default>
	
	<default type="progress">
		<texturebg colordiffuse="$VAR[color_universalcontainer_watchedoverlay_progressbar_remain]">colors/white.png</texturebg>
		<midtexture colordiffuse="$VAR[color_universalcontainer_watchedoverlay_progressbar_progress]">colors/white.png</midtexture>
		<lefttexture />
		<righttexture />
		<overlaytexture />
	</default>
	
	<default type="slider">
		<height>50</height>
		<texturesliderbar colordiffuse="$VAR[color_global_button_text_fo]" border="10">buttons/sliderback.png</texturesliderbar>
		<textureslidernib colordiffuse="$VAR[color_global_button_text_nf]">buttons/slidernib.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[color_global_button_selected]">buttons/slidernib.png</textureslidernibfocus>
		<orientation>horizontal</orientation>
	</default>
	<default type="sliderex">
		<height>50</height>
		<textoffsetx>40</textoffsetx>
		<aligny>center</aligny>
		<texturefocus colordiffuse="$VAR[color_global_button_panel_fo]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[color_global_button_panel_nf]">colors/white.png</texturenofocus>
		<sliderwidth>150</sliderwidth>
		<sliderheight>28</sliderheight>
		<texturesliderbar colordiffuse="$VAR[color_global_button_text_fo]">buttons/sliderback.png</texturesliderbar>
		<textureslidernib colordiffuse="$VAR[color_global_button_text_nf]">buttons/slidernib.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[color_global_button_selected]">buttons/slidernib.png</textureslidernibfocus>
		<font>font13</font>
		<focusedcolor>$VAR[color_global_button_text_fo]</focusedcolor>
		<textcolor>$VAR[color_global_button_text_nf]</textcolor>
		<disabledcolor>$VAR[color_global_button_disabled]</disabledcolor>
	</default>
	
	<default type="mover">
		<texturefocus colordiffuse="$VAR[color_global_button_panel_fo]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[color_global_button_panel_nf]">colors/white.png</texturenofocus>
	</default>
	<default type="resize">
		<texturefocus colordiffuse="$VAR[color_global_button_panel_fo]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[color_global_button_panel_nf]">colors/white.png</texturenofocus>
	</default>
	<default type="edit">
		<height>50</height>
		<texturefocus colordiffuse="$VAR[color_global_button_panel_fo]">colors/white.png</texturefocus>
		<texturenofocus colordiffuse="$VAR[color_global_button_panel_nf]">colors/white.png</texturenofocus>
		<font>font13</font>
		<focusedcolor>$VAR[color_global_button_text_fo]</focusedcolor>
		<textcolor>$VAR[color_global_button_text_nf]</textcolor>
		<disabledcolor>$VAR[color_global_button_disabled]</disabledcolor>
		<invalidcolor>$VAR[color_global_button_invalid]</invalidcolor>
		<textoffsetx>40</textoffsetx>
		<aligny>center</aligny>
		<align>left</align>
		<pulseonselect>no</pulseonselect>
	</default>
</includes>