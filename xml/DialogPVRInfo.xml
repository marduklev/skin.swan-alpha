	<defaultcontrol always="true">8000</defaultcontrol>
	<onload>ClearProperty(AdditionalInfo)</onload>
	<onload>RunScript(script.embuary.helper,action=resetposition,container=8601||8602||8603||8604||8605||8606||8607||8608||8609||8622)</onload>
	
