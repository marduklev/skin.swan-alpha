	</animation>
	<animation type="WindowClose">
		<effect type="fade" start="100" end="0" time="300" tween="sine" easing="out" />
		<effect type="zoom" center="50%,50%" start="100" end="0" time="300" tween="cubic" easing="out" />
	</animation>
   
