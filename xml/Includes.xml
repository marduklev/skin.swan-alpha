	<include file="swan_fonts.xml" />
	<include file="swan_expressions.xml" />
	<include file="swan_constants.xml" />
	<include file="swan_colors.xml" />
	<include file="swan_variables.xml" />
	<include file="swan_animations.xml" />
	<include file="swan_containerlayouts.xml" />
	<include file="swan_weather.xml" />
	<include file="swan_skinsettings.xml" />
	<include file="swan_backgroundbuilding.xml" />
	<include file="swan_menucontrol.xml" />
	<include file="swan_mediaflags.xml" />
	<include file="swan_player.xml" />
	<include file="swan_player_popups.xml" />
	<include file="swan_widget_shortcuts.xml" />
	<include file="swan_home.xml" />
	<include file="swan_home_widgets.xml" />
	<include file="swan_search.xml" />
	<include file="swan_hub.xml" />
	<include file="swan_pvr.xml" />
	<include file="swan_dialog_musicinfo.xml" />
	<include file="swan_dialog_videoinfo.xml" />
	<include file="swan_dialog_artworkslideshow.xml" />
	<include file="swan_views.xml" />
	<include file="swan_view_50_56.xml" />
	<include file="swan_view_51_52_53_54.xml" />
	<include file="swan_view_510_511_512_513.xml" />
	<include file="swan_view_58.xml" />
	<include file="swan_view_502.xml" />
	<include file="swan_view_503_504.xml" />
	<include file="swan_view_520.xml" />
	
	<include name="HiddenObject">
		<left>-3000</left>
		<top>-3000</top>
		<width>1</width>
		<height>1</height>
		<texturefocus />
		<texturenofocus />
		<label />
		<font />
	</include>
	<include name="HiddenContainer">
		<left>-3000</left>
		<top>-3000</top>
		<width>1</width>
		<height>1</height>
		<itemlayout height="10" width="10" />
		<focusedlayout height="10" width="10" />
	</include>
	
	<include name="FullScreenDimensions">
		<width>100%</width>
		<height>100%</height>
	</include>
	<include name="i_nested">
		<nested />
	</include>
	<include name="spacer_h">
		<param name="id" />
		<param name="gap" default="30" />
		<param name="visible" default="true" />
		<definition>
			<control type="label" id="$PARAM[id]">
				<visible>$PARAM[visible]</visible>
				<height>$PARAM[gap]</height>
				<font />
				<textcolor />
			</control>
		</definition>
	</include>
	<include name="spacer_w">
		<param name="id" />
		<param name="gap" default="30" />
		<param name="visible" default="true" />
		<definition>
			<control type="label" id="$PARAM[id]">
				<visible>$PARAM[visible]</visible>
				<width>$PARAM[gap]</width>
				<font />
				<textcolor />
			</control>
		</definition>
	</include>
	<include name="left_width">
		<left>$PARAM[left]</left><width>$PARAM[width]</width>
		<nested />
	</include>
	<include name="height">
		<height>$PARAM[height]</height>
	</include>
	<include name="width">
		<width>$PARAM[width]</width>
	</include>
	<include name="pos_left">
		<param name="left" default="0" />
		<definition>
			<left>$PARAM[left]</left>
		</definition>
	</include>
	<include name="pos_right">
		<param name="right" default="0" />
		<definition>
			<right>$PARAM[right]</right>
		</definition>
	</include>
	<include name="pos_alignx">
		<param name="align" default="left" />
		<definition>
			<align>$PARAM[align]</align>
		</definition>
	</include>
	<include name="pos_top">
		<top>$PARAM[top]</top>
	</include>
	
	<include name="page_attribute">
		<param name="top" default="0" />
		<definition>
			<top>$PARAM[top]</top>
			<height>1080</height>
		</definition>
	</include>
	
		
	
	<!-- leia version and below only ; reminder drop support sometime later and also lookup for System.BuildVersion condition; like string.startswith(System.BuildVersion,18.) -->
	<include name="lyrics_leia">
		<control type="label">
			<description>Pick label</description>
			<top>0</top>
			<height>70</height>
			<align>center</align>
			<aligny>center</aligny>
			<font>notosans_32_bold</font>
			<label>$ADDON[script.cu.lrclyrics 32005]</label>
			<textcolor>$VAR[color_osdmusic_main]</textcolor>
			<shadowcolor>black</shadowcolor>
			<visible>Control.IsVisible(120) + !Control.IsVisible(110)</visible>
		</control>
		<control type="list" id="120">
			<top>110</top>
			<width>100%</width>
			<height>80%</height>
			<onleft>121</onleft>
			<onright>121</onright>
			<onup>120</onup>
			<ondown>120</ondown>
			<pagecontrol>121</pagecontrol>
			<scrolltime>200</scrolltime>
			<animation effect="slide" start="0,0" end="20,0" time="0" condition="!Control.IsVisible(121)">Conditional</animation>
			<itemlayout height="40">
				<control type="label">
					<top>0</top>
					<height>40</height>
					<font>notosans_33_regular_l</font>
					<aligny>center</aligny>
					<label>$INFO[ListItem.Label]</label>
				</control>
			</itemlayout>
			<focusedlayout height="40">
				<control type="label">
					<left>10</left>
					<height>40</height>
					<font>notosans_33_regular_l</font>
					<aligny>center</aligny>
					<label>$INFO[ListItem.Label]</label>
					<textcolor>$VAR[color_osdmusic_accent]</textcolor>
				</control>
			</focusedlayout>
		</control>
	</include>
	
</includes>
