					<param name="visible" value="!$EXP[Scrollbar_Lib_Horizontal] + !String.IsEqual(Container.ViewMode,$LOCALIZE[31263])" />
					<param name="orientation" value="vertical" />
					<param name="panel_width" value="60" />
