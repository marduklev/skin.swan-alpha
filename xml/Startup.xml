<?xml version="1.0" encoding="UTF-8"?>
<window>
	<onload condition="!$EXP[content_not_locked]">Skin.ToggleSetting(skin_locked_views)</onload>
	<onload condition="Skin.HasSetting(TMDbHelper.Service)">Skin.Reset(TMDbHelper.Service)</onload>
	<onload condition="!Skin.HasSetting(startup_changelog_dontshow) | [Skin.HasSetting(startup_changelog_dontshow) + !String.IsEqual(skin.string(skinversion),1.17)]">ReplaceWindow(1190)</onload>
	<onload condition="Skin.HasSetting(startup_changelog_dontshow) + String.IsEqual(skin.string(skinversion),1.17)">ReplaceWindow($VAR[check_startup])</onload>
	<onload>Skin.SetString(skinversion,1.17)</onload>
	<controls />
</window>



	
