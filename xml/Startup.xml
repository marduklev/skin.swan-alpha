<?xml version="1.0" encoding="UTF-8"?>
<window>

	
	<!-- temp till next update, stay for a few -->
		<onload condition="!skin.string(shutdownmenuitem1_action)">Skin.SetString(shutdownmenuitem1_action,ActivateWindow(home))</onload>
		<onload condition="!skin.string(shutdownmenuitem1_name)">Skin.SetString(shutdownmenuitem1_name,$LOCALIZE[10000])</onload>
		<onload condition="!skin.string(shutdownmenuitem1_thumb)">Skin.SetString(shutdownmenuitem1_thumb,icons/home.png)</onload>
		
		<onload condition="!skin.string(shutdownmenuitem2_action)">Skin.SetString(shutdownmenuitem2_action,ActivateWindow(settings))</onload>
		<onload condition="!skin.string(shutdownmenuitem2_name)">Skin.SetString(shutdownmenuitem2_name,$LOCALIZE[5])</onload>
		<onload condition="!skin.string(shutdownmenuitem2_thumb)">Skin.SetString(shutdownmenuitem2_thumb,icons/settingsslider.png)</onload>
		
		<onload condition="!skin.string(shutdownmenuitem3_action)">Skin.SetString(shutdownmenuitem3_action,quit())</onload>
		<onload condition="!skin.string(shutdownmenuitem3_name)">Skin.SetString(shutdownmenuitem3_name,$LOCALIZE[13012])</onload>
		<onload condition="!skin.string(shutdownmenuitem3_thumb)">Skin.SetString(shutdownmenuitem3_thumb,icons/arrowleft.png)</onload>
		
		<onload condition="!skin.string(shutdownmenuitem4_action) + System.CanPowerDown">Skin.SetString(shutdownmenuitem4_action,powerdown())</onload>
		<onload condition="!skin.string(shutdownmenuitem4_name) + System.CanPowerDown">Skin.SetString(shutdownmenuitem4_name,$LOCALIZE[13016])</onload>
		<onload condition="!skin.string(shutdownmenuitem4_thumb) + System.CanPowerDown">Skin.SetString(shutdownmenuitem4_thumb,icons/shutdown.png)</onload>
		
		<onload condition="!skin.string(shutdownmenuitem5_action) + System.CanReboot">Skin.SetString(shutdownmenuitem5_action,reboot())</onload>
		<onload condition="!skin.string(shutdownmenuitem5_name) + System.CanReboot">Skin.SetString(shutdownmenuitem5_name,$LOCALIZE[13016])</onload>
		<onload condition="!skin.string(shutdownmenuitem5_thumb) + System.CanReboot">Skin.SetString(shutdownmenuitem5_thumb,icons/shittyrebbot.png)</onload>
		
		<onload condition="!skin.string(shutdownmenuitem6_action) + System.CanHibernate">Skin.SetString(shutdownmenuitem6_action,hibernate())</onload>
		<onload condition="!skin.string(shutdownmenuitem6_name) + System.CanHibernate">Skin.SetString(shutdownmenuitem6_name,$LOCALIZE[13010])</onload>
		<onload condition="!skin.string(shutdownmenuitem6_thumb) + System.CanHibernate">Skin.SetString(shutdownmenuitem6_thumb,icons/hibernate.png)</onload>
		
		<onload condition="!skin.string(shutdownmenuitem7_action) + System.CanSuspend">Skin.SetString(shutdownmenuitem7_action,suspend())</onload>
		<onload condition="!skin.string(shutdownmenuitem7_name) + System.CanSuspend">Skin.SetString(shutdownmenuitem7_name,$LOCALIZE[13011])</onload>
		<onload condition="!skin.string(shutdownmenuitem7_thumb) + System.CanSuspend">Skin.SetString(shutdownmenuitem7_thumb,icons/suspend.png)</onload>
		
		<onload condition="!skin.string(shutdownmenuitem8_action)">Skin.SetString(shutdownmenuitem8_action,alarmclock(shutdowntimer,shutdown()))</onload>
		<onload condition="!skin.string(shutdownmenuitem8_name)">Skin.SetString(shutdownmenuitem8_name,$LOCALIZE[20180])</onload>
		<onload condition="!skin.string(shutdownmenuitem8_thumb)">Skin.SetString(shutdownmenuitem8_thumb,icons/alarmadd.png)</onload>
				
		<onload condition="!skin.string(shutdownmenuitem9_action)">Skin.SetString(shutdownmenuitem9_action,reload.skin())</onload>
		<onload condition="!skin.string(shutdownmenuitem9_name)">Skin.SetString(shutdownmenuitem9_name,reload skin)</onload>
		<onload condition="!skin.string(shutdownmenuitem9_thumb)">Skin.SetString(shutdownmenuitem9_thumb,resource://resource.images.skin.swan/tempfoldersqaurethumbs/synchronize.png)</onload>
		
	
	<onload condition="!$EXP[content_not_locked]">Skin.ToggleSetting(skin_locked_views)</onload>
	<onload condition="Skin.HasSetting(TMDbHelper.Service)">Skin.Reset(TMDbHelper.Service)</onload>
	<onload>ReplaceWindow($VAR[check_startup])</onload>
	<controls />
</window>

 