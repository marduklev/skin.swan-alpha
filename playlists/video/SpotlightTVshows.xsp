<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>Spotlight TV Shows</name>
    <match>one</match>
    <rule field="rating" operator="greaterthan">
        <value>6.9</value>
    </rule>
    <rule field="numwatched" operator="is">
        <value>0</value>
    </rule>
    <rule field="dateadded" operator="inthelast">
        <value>60 days</value>
    </rule>
    <limit>100</limit>
    <order direction="ascending">random</order>
</smartplaylist>
